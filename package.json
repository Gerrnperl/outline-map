{
	"name": "outline-map",
	"displayName": "Outline Map",
	"description": "A visual, interactive outline map. Alternative Minimap.",
	"version": "0.2.1",
	"repository": {
		"url": "https://github.com/Gerrnperl/outline-map.git"
	},
	"keywords": [
		"outline",
		"map",
		"minimap",
		"Thumbnail"
	],
	"author": {
		"name": "Gerrnperl"
	},
	"browser": "./out/extension.js",
	"icon": "icon/outline-map.png",
	"publisher": "Gerrnperl",
	"galleryBanner": {
		"color": "#005487",
		"theme": "dark"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onView:outline-map-view"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "outline-map",
					"title": "Outline Map",
					"icon": "icon/outline-map-24.svg"
				}
			]
		},
		"views": {
			"outline-map": [
				{
					"type": "webview",
					"id": "outline-map-view",
					"name": "Outline Map"
				}
			]
		},
		"configuration": {
			"title": "Outline Map",
			"properties": {
			  "outline-map.color": {
				"type": "object",
				"default": {},
				"description": "color table for specific symbols.",
				"properties": {
					"module": {
						"type": "string",
						"default": "",
						"description": "Module symbol color"
					},
					"namespace": {
						"type": "string",
						"default": "",
						"description": "Namespace symbol color"
					},
					"package": {
						"type": "string",
						"default": "",
						"description": "Package symbol color"
					},
					"class": {
						"type": "string",
						"default": "",
						"description": "Class symbol color"
					},
					"method": {
						"type": "string",
						"default": "",
						"description": "Method symbol color"
					},
					"property": {
						"type": "string",
						"default": "",
						"description": "Property symbol color"
					},
					"field": {
						"type": "string",
						"default": "",
						"description": "Field symbol color"
					},
					"constructor": {
						"type": "string",
						"default": "",
						"description": "Constructor symbol color"
					},
					"enum": {
						"type": "string",
						"default": "",
						"description": "Enum symbol color"
					},
					"interface": {
						"type": "string",
						"default": "",
						"description": "Interface symbol color"
					},
					"function": {
						"type": "string",
						"default": "",
						"description": "Function symbol color"
					},
					"variable": {
						"type": "string",
						"default": "",
						"description": "Variable symbol color"
					},
					"constant": {
						"type": "string",
						"default": "",
						"description": "Constant symbol color"
					},
					"string": {
						"type": "string",
						"default": "",
						"description": "String symbol color"
					},
					"number": {
						"type": "string",
						"default": "",
						"description": "Number symbol color"
					},
					"boolean": {
						"type": "string",
						"default": "",
						"description": "Boolean symbol color"
					},
					"array": {
						"type": "string",
						"default": "",
						"description": "Array symbol color"
					},
					"object": {
						"type": "string",
						"default": "",
						"description": "Object symbol color"
					},
					"null": {
						"type": "string",
						"default": "",
						"description": "Null symbol color"
					},
					"enumMember": {
						"type": "string",
						"default": "",
						"description": "EnumMember symbol color"
					},
					"struct": {
						"type": "string",
						"default": "",
						"description": "Struct symbol color"
					},
					"event": {
						"type": "string",
						"default": "",
						"description": "Event symbol color"
					},
					"operator": {
						"type": "string",
						"default": "",
						"description": "Operator symbol color"
					},
					"typeParameter": {
						"type": "string",
						"default": "",
						"description": "TypeParameter symbol color"
					}
				}
			  }
			}
		  }
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/mocha": "^9.1.1",
		"eslint": "^8.18.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"@vscode/test-web": "^0.0.26",
		"ts-loader": "^9.3.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"@types/webpack-env": "^1.17.0",
		"assert": "^2.0.0",
		"process": "^0.11.10"
	},
	"dependencies": {
		"@vscode/codicons": "^0.0.31",
		"@vscode/webview-ui-toolkit": "^1.0.0"
	}
}