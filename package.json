{
	"name": "outline-map",
	"displayName": "Outline Map",
	"description": "A visual, interactive outline map. Alternative Minimap.",
	"version": "0.0.3",
	"repository": {
		"url": "https://github.com/Gerrnperl/outline-map.git"
	},
	"keywords": [
		"outline",
		"map",
		"minimap",
		"Thumbnail"
	],
	"author": {
		"name": "Gerrnperl"
	},
	"browser": "./out/extension.js",
	"icon": "icon/outline-map.png",
	"publisher": "Gerrnperl",
	"galleryBanner": {
		"color": "#005487",
		"theme": "dark"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "outline-map",
					"title": "Outline Map",
					"icon": "icon/outline-map-24.svg"
				}
			]
		},
		"views": {
			"outline-map": [
				{
					"type": "webview",
					"id": "outline-map-view",
					"name": "Outline Map"
				}
			]
		}
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/mocha": "^9.1.1",
		"eslint": "^8.18.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"@vscode/test-web": "^0.0.26",
		"ts-loader": "^9.3.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"@types/webpack-env": "^1.17.0",
		"assert": "^2.0.0",
		"process": "^0.11.10"
	},
	"dependencies": {
		"@vscode/codicons": "^0.0.31",
		"@vscode/webview-ui-toolkit": "^1.0.0"
	}
}